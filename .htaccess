AddDefaultCharset utf-8
DirectoryIndex index.php

Options +FollowSymLinks
RewriteEngine On
RewriteCond "%{REQUEST_URI}" "!^(/|/index\.php|/test\.php)$"
RewriteCond "%{REQUEST_URI}" "\.[^\.]+$"
RewriteCond "%{REQUEST_URI}" "!\.(js|css|jpg|woff2)$"
RewriteRule ".*" "-" [F,L]

<FilesMatch "\.(js|css|jpg|woff2)$">
    Header set Cache-Control "max-age=86400"
</FilesMatch>
<FilesMatch "\.(js|css)$">
    SetOutputFilter DEFLATE
</FilesMatch>

Header unset Server
Header unset X-Powered-By


